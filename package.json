{
  "name": "cozyartz-media-group",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "VITE_ENVIRONMENT=production vite build",
    "build:pages": "VITE_ENVIRONMENT=production vite build && cp public/_routes.json dist/ && cp public/_redirects dist/",
    "build:spa": "VITE_ENVIRONMENT=production vite build && cp public/_routes.json dist/ && cp public/_redirects dist/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test:routing": "node test-routing.mjs",
    "test:supabase": "node test-supabase-auth.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:roles": "node test-role-routing.mjs",
    "test:pricing": "node test-pricing-page.mjs",
    "prepare": "husky install",
    "worker:dev": "wrangler dev worker.js --port 8787",
    "worker:deploy": "wrangler deploy",
    "worker:deploy:staging": "wrangler deploy --env staging",
    "worker:deploy:dev": "wrangler deploy --env development",
    "deploy": "VITE_ENVIRONMENT=production VITE_TURNSTILE_SITE_KEY=0x4AAAAAABlo_LdXn1ErLBXD npm run build:pages && npm run deploy:worker && npm run deploy:pages",
    "deploy:worker": "wrangler deploy --env=\"\"",
    "deploy:pages": "wrangler pages deploy dist",
    "deploy:staging": "npm run build && wrangler deploy --env staging && wrangler pages deploy dist",
    "deploy:production": "VITE_ENVIRONMENT=production VITE_TURNSTILE_SITE_KEY=0x4AAAAAABlo_LdXn1ErLBXD npm run build:pages && wrangler deploy --env=\"\" && wrangler pages deploy dist"
  },
  "dependencies": {
    "@cloudflare/ai": "^1.2.2",
    "@cloudflare/workers-types": "^4.20250715.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@paypal/paypal-js": "^8.2.0",
    "@paypal/react-paypal-js": "^8.8.3",
    "@supabase/supabase-js": "^2.52.0",
    "dotenv": "^17.2.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^7.6.3",
    "resend": "^4.6.0",
    "wrangler": "^4.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.0.1",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
