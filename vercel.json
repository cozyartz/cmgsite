{
  "rewrites": [
    {
      "source": "/ai-services",
      "destination": "/ai-services.html"
    },
    {
      "source": "/seo-services", 
      "destination": "/seo-services.html"
    },
    {
      "source": "/instructional-design-services",
      "destination": "/instructional-design-services.html"
    },
    {
      "source": "/multimedia-services",
      "destination": "/multimedia-services.html"
    },
    {
      "source": "/drone-services",
      "destination": "/drone-services.html"
    },
    {
      "source": "/web-graphic-design-services",
      "destination": "/web-graphic-design-services.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}